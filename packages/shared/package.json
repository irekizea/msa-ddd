{
    "name": "@msaddd/shared",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
        "build:js": "tsup src/index.ts --format esm,cjs --clean --tsconfig tsconfig.build.json",
        "build": "pnpm run build:types && pnpm run build:js",
        "dev": "tsup src/index.ts --format esm,cjs --watch --tsconfig tsconfig.build.json"
    },
    "dependencies": {
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    }
}